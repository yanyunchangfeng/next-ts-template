{
  "name": "next-ts-template",
  "version": "1.5.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "dev:test": "cross-env ENV=test next dev",
    "dev:debug": "NODE_OPTIONS=--inspect next dev",
    "build": "next build",
    "build:export": "cross-env NEXT_PUBLIC_BUILD_MODE=export NEXT_PUBLIC_BUILD_PATH=/next-ts-template/ NEXT_PUBLIC_BASE_PATH=/next-ts-template NEXT_PUBLIC_DIST_DIR=dist next build",
    "start": "next build && next start",
    "lint": "next lint",
    "test": "jest",
    "prettier": "prettier -c --write  **/* --cache",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "release": "semantic-release"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yanyunchangfeng/next-ts-template.git"
  },
  "dependencies": {
    "next": "14.2.14",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "semantic-release": "24.1.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.1",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "husky": "8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "13.2.3",
    "postcss": "^8",
    "prettier": "2.8.8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "prettier --write  --ignore-unknown",
    "*.{json,less,scss,md}": "prettier --write --ignore-unknown"
  }
}
